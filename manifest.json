{
  "manifest_version": 3,
  "name": "YouTube Ad Bypass",
  "version": "1.0",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["*://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_title": "Bypass",
    "default_icon": {
      "16": "./bypass.png",
      "48": "./bypass.png",
      "128": "./bypass.png"
    }
  },
  "icons": {
    "16": "./block.png",
    "48": "./block.png",
    "128": "./block.png"
  }
}
